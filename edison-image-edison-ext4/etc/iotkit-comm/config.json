{
    "pluginInterfaceDir": "plugin-interfaces",
    "pluginDir": "plugins",
    "superInterfaceName": "interface-for-interfaces",
    "serviceDir": "serviceSpecs",
    "communication": {
        "pluginFileSuffixes": {
            "clientFileSuffix": "client",
            "serverFileSuffix": "service"
        }
