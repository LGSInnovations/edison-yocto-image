        "CAName": "myca",
        "privateKeyNameSuffix": "_key",
        "hostKeyNameSuffix": "_host",
        "clientKeyNameSuffix": "_client",
        "SSLCertSuffix": ".crt",
        "SSLCertReqSuffix": ".csr",
        "SSHPubKeySuffix": ".pub",
        "defaultUser": "root",
        "SSHCertSuffix": "-cert"
